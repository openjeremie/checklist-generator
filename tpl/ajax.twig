{% for itemcl in checklists %}

<tr class="{% if itemcl.label is defined %} {{ itemcl.label.classbg }} {% endif %} delete-{{ itemcl.id }} line" id="{{ itemcl.id }}" data-toggle="modal">

<td><i class="icon-{{ itemcl.icon }}"></td>
<td>{{ itemcl.name }}
{% if itemcl.label is defined %}
<span class="label label-{{ itemcl.label.class }}">
{{ itemcl.label.status }}
</span>
{% endif %}
</td>
<td>{{ itemcl.departement }}</td>
<td class="complete-case">
	<div class="row-fluid row-complete">
			{% for status in itemcl.completeClass %}
			<div class="span4"><i class="icon-{{ status }}"></i></div>
			{% endfor %}
	</div>
</td>
<td>{{ itemcl.timeLastUpdate }}</td>
<td>{{ itemcl.link }}</td>
<td data-action="edit" id="edit-{{ itemcl.id }}"><i class="icon-list"></i></td>
<td data-action="delete" id="delete-{{itemcl.id }}"><button data-toggle="modal" class="delete-row-bt close">&times;</button></td>
</tr>
{% endfor %}

